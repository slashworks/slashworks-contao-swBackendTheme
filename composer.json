{
  "name": "slashworks/contao-swbackend-theme",
  "type": "contao-bundle",
  "description": "Contao Cookie Manager",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "slashworks",
      "homepage": "https://github.com/slashworks"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4",
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "extra": {
    "contao-manager-plugin": "Slashworks\\ContaoSwBackendThemeBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Slashworks\\ContaoSwBackendThemeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slashworks\\ContaoSwBackendThemeBundle\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/slashworks/slashworks-contao-swBackendTheme/issues",
    "source": "https://github.com/slashworks/slashworks-contao-swBackendTheme"
  }
}
